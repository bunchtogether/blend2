{
  "name": "blend-runtime",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "bin": "index.js",
  "scripts": {
    "pm2": "pm2",
    "start": "node index.js",
    "build": "npm run build:winx64 && npm run build:winx86",
    "build:winx64": "pkg --output blend-runtime-x64.exe --target node10-win-x64 index.js",
    "build:winx86": "pkg --output blend-runtime-x86.exe --target node10-win-x86 index.js"
  },
  "dependencies": {
    "@bunchtogether/exit-handler": "^1.0.0",
    "@bunchtogether/node-systray": "^1.1.0",
    "pm2": "^3.5.1"
  },
  "devDependencies": {
    "pkg": "^4.4.0"
  }
}
