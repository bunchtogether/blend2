[ignore]

.*/node_modules/immutable/dist/.*.flow
.*/node_modules/.*/node_modules/fbjs/.*
.*/node_modules/jss/.*
.*/node_modules/.*/node_modules/.*
.*/node_modules/.*/test/.*
.*/node_modules/.*/tests/.*

[include]
../vendor/

[libs]

flow-typed/
flow/

[options]

module.ignore_non_literal_requires=true
module.name_mapper='.*\(.scss\)' -> 'empty/object'
module.name_mapper='shared-redux' -> '<PROJECT_ROOT>/app/vendor/shared-redux'
module.name_mapper='components' -> '<PROJECT_ROOT>/app/components'
module.name_mapper='containers' -> '<PROJECT_ROOT>/app/containers'
module.name_mapper='boost-client' -> '<PROJECT_ROOT>/app/vendor/boost-client'
module.name_mapper='utils' -> '<PROJECT_ROOT>/app/utils'
module.name_mapper='ipfs' -> 'empty/object'
module.name_mapper='@bunchtogether/blend2-client' -> '<PROJECT_ROOT>/../vendor/client'

[version]

